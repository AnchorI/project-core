{
  "name": "evcar-core",
  "version": "1.0.0",
  "description": "Server side for evcar",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "start": "tsc && cross-env NODE_ENV=development nodemon",
    "dev": "cross-env NODE_ENV=development nodemon",
    "lint:all": "yarn lint && yarn tscCheck",
    "lint:fix": "eslint src/**/*.ts --fix",
    "lint": "./node_modules/.bin/eslint ./src --ext .ts  --quiet",
    "type": "tsc --project tsconfig.json --noEmit",
    "format": "prettier --write 'src/**/*.{js,ts,json}'",
    "seed:test": "cross-env NODE_ENV=test ts-node --transpile-only --files src/database/seeder.ts",
    "apidoc": "ts-node --files src/commands/apidoc/index.ts",
    "apidoc:dev": "cross-env NODE_ENV=development nodemon --exec ts-node --transpile-only src/processes/apidoc.ts"
  },
  "keywords": [],
  "author": "AnchorI",
  "license": "ISC",
  "repository": "https://git.aftermath.team/AnchorI/evcar-node",
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/async": "^3.2.16",
    "@types/clone-deep": "^4.0.1",
    "@types/cors": "^2.8.12",
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.14",
    "@types/http-status": "^1.1.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.11.10",
    "@types/node-emoji": "^1.8.2",
    "@types/prettier": "^2.7.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "async": "^3.2.4",
    "clone-deep": "^4.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.6",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "faker-js": "^1.0.0",
    "fishery": "^2.2.2",
    "global": "^4.4.0",
    "helpers": "^0.0.6",
    "http-status": "^1.5.3",
    "jest": "^29.3.1",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^13.0.4",
    "module-alias": "^2.2.2",
    "node-emoji": "^1.11.0",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "pino": "^8.7.0",
    "pm2": "^5.2.2",
    "prettier": "^2.8.0",
    "save-dev": "^0.0.1-security",
    "sequelize": "^6.26.0",
    "swagger-parser": "^10.0.3",
    "swagger-ui-express": "^4.6.0",
    "typescript": "^4.9.3",
    "yamljs": "^0.3.0"
  },
  "simple-pre-commit": "npx lint-staged"
}
